{
  "name": "@boardmeister/antetype-workspace",
  "version": "0.0.4",
  "description": "Antetype module for defining workspace and related features",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "typescript": "./node_modules/.bin/dts-bundle-generator --no-check --export-referenced-types false --project tsconfig.json -o dist/index.d.ts src/index.ts",
    "build": "node esbuild.config.mjs && npm run typescript",
    "test": "npx jasmine-browser-runner runSpecs",
    "serve-jasmine": "npx jasmine-browser-runner serve"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Board-Meister/antetype-workspace.git"
  },
  "keywords": [
    "module",
    "frontend",
    "tool"
  ],
  "author": "",
  "license": "GPL-2.0-only",
  "bugs": {
    "url": "https://github.com/Board-Meister/antetype-workspace/issues"
  },
  "homepage": "https://github.com/Board-Meister/antetype-workspace#readme",
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/jasmine": "^5.1.8",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "autoprefixer": "^10.4.20",
    "dts-bundle-generator": "^9.5.1",
    "esbuild": "0.21.4",
    "esbuild-plugin-inline-import": "^1.0.4",
    "esbuild-plugin-output-reset": "^2.0.1",
    "eslint": "^9.9.0",
