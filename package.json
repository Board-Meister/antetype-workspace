    "jasmine": "^5.7.1",
    "jasmine-browser-runner": "^3.0.0",
    "jasmine-core": "^5.7.1",
    "postcss": "^8.4.38",
    "terser": "^5.31.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "esbuild-copy-static-files": "^0.1.0"
  },
  "dependencies": {
    "@boardmeister/antetype-conditions": "^0.0.4",
    "@boardmeister/antetype-core": "^0.0.5",
    "@boardmeister/antetype-cursor": "^0.0.5",
    "@boardmeister/herald": "^0.0.1",
    "@boardmeister/marshal": "^0.0.2"
